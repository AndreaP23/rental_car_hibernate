<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jwt.hibernate.bean.Veicolo" table="VEICOLO">
        <id name="id" column="veicolo_id">
            <generator class="native"/>
        </id>
        <property column="marca" name="marca" type="java.lang.String" />
        <property column="modello" name="modello" type="java.lang.String" />
        <property column="anno" name="anno" type="java.lang.Integer" />
        <property column="targa" name="targa" type="java.lang.String" />
        <property column="disponibilita" name="disponibilita" type="java.lang.String" />
    </class>
</hibernate-mapping>
